{
  "name": "stream-deck-browser",
  "productName": "Stream Deck Browser",
  "version": "0.0.5",
  "description": "A web browser running on your Elgato Stream Deck",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test node --trace-warnings ./test/runTests.js",
    "test-all": "npm run test && npm run build && npm run test-e2e",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test node --trace-warnings ./test/runTests.js e2e",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 server.js",
    "build-main": "cross-env NODE_ENV=production tsc -p tsconfig.electron-main.json && copy .\\app\\main.development.js .\\app\\main.js",
    "build-renderer": "cross-env NODE_ENV=production node ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "electron-rebuild": "electron-rebuild && electron-rebuild --module-dir ./app/",
    "clean": "del .\\app\\main.development.js",
    "build": "npm run clean && npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron --inspect ./app/",
    "start-hot": "tsc -p tsconfig.electron-main.json && cross-env HOT=1 NODE_ENV=development electron --inspect ./app/main.development",
    "postinstall": "npm run build",
    "dev": "npm run hot-server -- --start-hot",
    "package": "npm run build && electron-builder --publish never",
    "package-win": "npm run build && electron-builder --win --x64",
    "package-linux": "npm run build && electron-builder --linux",
    "package-all": "npm run build && electron-builder -mwl",
    "cleanup": "mop -v"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/node_modules"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test/preprocessor.js"
    },
    "testMatch": [
      "**/?(*.)(spec|test).ts?(x)"
    ]
  },
  "build": {
    "productName": "Stream Deck Browser",
    "appId": "io.github.jstarpl.stream-deck-browser",
    "asar": true,
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "main.js",
      "main.js.map",
      "package.json",
      "build/",
      "icon.png",
      "api/**/*.js"
    ],
    "extraFiles": [
      {
        "from": "node_modules/node-hid/build/Release/HID.node",
        "to": "build/HID.node"
      },
      {
        "from": "node_modules/@julusian/jpeg-turbo/build/Release/jpegturbo.node",
        "to": "build/jpegturbo.node"
      }
    ],
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "win": {
      "target": "nsis",
      "publish": [
        "github"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jstarpl/stream-deck-browser.git"
  },
  "authors": [
    {
      "name": "Jan Starzak",
      "email": "jan.starzak@gmail.com",
      "url": "https://github.com/jstarpl"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jstarpl/stream-deck-browser/issues"
  },
  "keywords": [
    "stream-deck",
    "browser",
    "html",
    "electron",
    "react"
  ],
  "homepage": "https://github.com/jstarpl/stream-deck-browser#readme",
  "devDependencies": {
    "@types/electron-settings": "^3.1.1",
    "@types/enzyme": "^3.1.1",
    "@types/history": "^4.5.2",
    "@types/jest": "^22.0.0",
    "@types/node": "^8.0.2",
    "@types/react": "^16.0.5",
    "@types/react-dom": "16.0.3",
    "@types/react-hot-loader": "^3.0.4",
    "@types/react-redux": "^5.0.4",
    "@types/react-router": "^4.0.11",
    "@types/react-router-dom": "^4.0.7",
    "@types/react-router-redux": "^5.0.2",
    "@types/redux-logger": "^3.0.0",
    "@types/sinon": "^4.0.0",
    "asar": "^0.14.0",
    "boiler-room-custodian": "^0.6.2",
    "concurrently": "^3.1.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "css-modules-require-hook": "^4.0.6",
    "devtron": "^1.4.0",
    "electron": "^7.2.4",
    "electron-builder": "^22.7.0",
    "electron-builder-http": "^19.15.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-rebuild": "^1.8.5",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.24.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.0.4",
    "json-loader": "^0.5.4",
    "native-ext-loader": "^2.3.0",
    "node-sass": "^4.1.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^16.0.0",
    "redux-logger": "^3.0.6",
    "sass-loader": "^6.0.6",
    "sinon": "^4.0.0",
    "spectron": "^3.4.1",
    "style-loader": "^0.19.0",
    "ts-loader": "^3.1.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.4.3",
    "typescript": "3.8.3",
    "url-loader": "^0.6.1",
    "webpack": "^3.3.0",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "@julusian/jpeg-turbo": "0.5.4",
    "electron-debug": "^1.1.0",
    "history": "^4.6.1",
    "node-hid": "1.3.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.1",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "source-map-support": "^0.5.19"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  },
  "resolutions": {
    "minimist": "^0.2.1",
    "kind-of": "^6.0.3",
    "js-yaml": "^3.13.1"
  }
}
